{
    "name": "haizad/laravel-keycloak-admin",
    "description": "This package supports Keycloak administrator client API for Laravel 8",
    "require": {
        "guzzlehttp/guzzle": "^7.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "mnikoei",
            "email": "mohsen.nikoei@gmail.com",
            "homepage": "https://github.com/Mnikoei",
            "role": "developer creator"
        },
        {
            "name": "Haizad",
            "email": "haizadannuar@gmail.com",
            "homepage": "https://github.com/haizad",
            "role": "contributing developer"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "LaravelKeycloakAdmin\\": "src/"
        },
        "files": [
            "src/lumen-helpers.php"
        ]
    },
    "extra" : {
        "laravel" : {
            "providers" : [
                "LaravelKeycloakAdmin\\KeycloakAdminServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "mkdir -p config"
        ]
    }
}
